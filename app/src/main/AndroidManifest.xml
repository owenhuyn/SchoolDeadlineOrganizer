<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.owenhuyn.schooldeadlineorganizer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".courseDetail"
            android:label="@string/title_activity_course_detail" >
        </activity>
        <activity
            android:name=".DrawerAdapter"
            android:label="@string/title_activity_drawer_adapter" >
        </activity>
    </application>

</manifest>
